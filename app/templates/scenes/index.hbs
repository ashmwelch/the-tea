<h2>List</h2>

{{#each model as |scenery|}}
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">{{scenery.name}}</h3>
    </div>
    <div class="panel-body">
      <p>Address: {{scenery.address}}</p>
      <p>Phone: {{scenery.phone}}</p>
    </div>
  </div>
{{/each}}
